{
  "name": "theme-sapphire",
  "displayName": "Sapphire Theme",
  "description": "Sapphire is a vibrant blue theme for Visual Studio Code",
  "version": "0.2.0",
  "publisher": "Tyriar",
  "icon": "images/icon.png",
  "license": "MIT",
  "scripts": {
    "prestart": "tsc index.ts",
    "start": "node index.js"
  },
  "bugs": {
    "url": "https://github.com/Tyriar/vscode-theme-sapphire/issues"
  },
  "homepage": "https://github.com/Tyriar/vscode-theme-sapphire",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tyriar/vscode-theme-sapphire"
  },
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Sapphire",
        "uiTheme": "vs-dark",
        "path": "./theme.json"
      },
      {
